{
	"name": "vault-to-blog",
	"version": "0.0.6",
	"description": "An Obsidian plugin to publish an Obsidian vault or directory to a GitHub Pages blog.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json ./react-app-internal/package.json ./react-app-internal/package-lock.json"
	},
	"keywords": [
		"obsidian",
		"github",
		"github page",
		"plugin",
		"blog",
		"obsidian plugin"
	],
	"author": "barkstone2",
	"license": "MIT",
	"devDependencies": {
		"@types/adm-zip": "^0.5.5",
		"@types/awesomplete": "^1.1.15",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"adm-zip": "^0.5.15",
		"simple-git": "^3.16.0"
	}
}
